			<interface.java interface="org.loushang.bsp.permit.pap.function.dao.IAppDao" />
			<binding.sca />
		</service>
		<property name="dataSourceConf" value="dataSource" />
		<implementation.java
			impl="org.loushang.bsp.permit.pap.function.dao.jdbc.AppJdbcDao" />
	</component>
	
